#!/bin/bash

OS=`/usr/sbin/sysctl -n kern.osversion`
PROD=`/usr/sbin/sysctl -n hw.machine`

case $PROD in
  iPhone4,1 | iPad2* | iPad3,[123] | iPod5,1 )
    case $OS in
      12[ABD]* ) :;;
      * )
        echo "For this device/version, use EverUntether Migrator instead."
        exit 1
      ;;
    esac
  ;;
  * )
    if [[ $OS == "12"* ]]; then
      echo "For this device/version, use EverUntether Migrator instead."
      exit 1
    fi
  ;;
esac

rm -rf /.installed_everpwnage /.installed-openpwnage /.installed_home_depot
