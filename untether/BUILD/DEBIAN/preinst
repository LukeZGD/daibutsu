#!/bin/bash

if [[ -d /untether ]]; then
    echo "EtasonJB detected. Removing files..."
    rm -rf /usr/libexec/rtbuddyd /--early-boot /untether \
       /private/etc/apt/sources.list.d/net.tihmstar.list \
       /private/etc/rc.d/daemonload /usr/bin/orphan_commander \
       /private/var/lib/dpkg/info/net.tihmstar.etasonuntether*
    echo "Done removing files. Install EverUntether again to complete migration."
    echo "You may also now remove the Etason untether package."
    exit 1
fi
rm -rf /.installed_everpwnage /.installed-openpwnage /.installed_home_depot
